{
    "console": {
        "colorize": true,
        "timestamp": true,
        "level": "debug",
        "stderrLevels": [
            "error",
            "debug",
            "info"
        ]
    },
    "file": {
        "colorize": false,
        "json": false,
        "filename": "logs/application.log",
        "datePattern": "yyyy-MM-dd.",
        "prepend": true,
        "level": "info"
    }
}